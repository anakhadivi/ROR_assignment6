<h1>Edit Your Account</h1>
<%=form_for @user do |f| %>
	<%= f.label :fname, 'First Name' %>
	<%= f.text_field :fname %>
	<%= f.label :lname, 'Last Name' %>
	<%= f.text_field :lname %>
	<%= f.label :email, 'Email' %>
	<%= f.text_field :email %>
	<%=f.label :password, 'Password' %>
	<%= f.password_field :password %>
	<%= f.submit :'Update my account' %>
<% end %>